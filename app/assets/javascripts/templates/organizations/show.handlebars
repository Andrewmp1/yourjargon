<div id="showOrganizationView" class='well'>
    <h1> {{content.name}} </h1>
    Memberships: {{content.memberships.length}} <br />
       Members:
       <ul>
       {{#each membership in content.memberships}}
         <li>{{membership.user.email}} <button>Remove</button></li>
       {{/each}}
       </ul>
    <br />
    Terms: {{content.terms.length}}&nbsp;<button {{action newTerm}}> New Term </button>
        {{outlet orgTerms}}
</div>
